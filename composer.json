{
    "name": "projet-normandie/symfony-skeleton",
    "description": "Symfony Skeleton Project",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "api-platform/doctrine-orm": "^4.1",
        "api-platform/graphql": "^4.1",
        "api-platform/symfony": "^4.1",
        "aws/aws-sdk-php-symfony": "*",
        "damienharper/auditor-bundle": "^6.2",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.3",
        "gesdinet/jwt-refresh-token-bundle": "^1.4",
        "google/apiclient": "^2.18",
        "league/flysystem-aws-s3-v3": "*",
        "league/flysystem-bundle": "^3.4",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.1",
        "projet-normandie/article-bundle": "dev-develop",
        "projet-normandie/forum-bundle": "dev-develop",
        "projet-normandie/message-bundle": "dev-develop",
        "projet-normandie/twitch-bundle": "dev-develop",
        "projet-normandie/user-bundle": "dev-develop",
        "sonata-project/admin-bundle": "^4.37",
        "sonata-project/doctrine-orm-admin-bundle": "^4.18",
        "sonata-project/translation-bundle": "^4.x-dev",
        "sonata-project/user-bundle": "^5.14",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/amazon-mailer": "7.3.*",
        "symfony/asset": "^7.3",
        "symfony/brevo-mailer": "^7.3",
        "symfony/console": "^7.3",
        "symfony/doctrine-messenger": "^7.3",
        "symfony/dotenv": "^7.3",
        "symfony/event-dispatcher": "^7.3",
        "symfony/expression-language": "^7.3",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7.3",
        "symfony/http-client": "^7.3",
        "symfony/mailer": "^7.3",
        "symfony/messenger": "^7.3",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "^7.3",
        "symfony/property-info": "^7.3",
        "symfony/rate-limiter": "7.3.*",
        "symfony/runtime": "^7.3",
        "symfony/security-bundle": "^7.3",
        "symfony/serializer": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/validator": "^7.3",
        "symfony/yaml": "^7.3",
        "video-games-records/core-bundle": "dev-develop",
        "video-games-records/dwh-bundle": "dev-develop",
        "yokai/sonata-workflow": "*",
        "zenstruck/messenger-monitor-bundle": "^0.5.2"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.3",
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "squizlabs/php_codesniffer": "^3.12",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/phpunit-bridge": "^7.3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "ProjetNormandie\\ArticleBundle\\": "../../bundles/projet-normandie/article-bundle/src",
            "ProjetNormandie\\UserBundle\\": "../../bundles/projet-normandie/user-bundle/src",
            "ProjetNormandie\\ForumBundle\\": "../../bundles/projet-normandie/forum-bundle/src",
            "ProjetNormandie\\MessageBundle\\": "../../bundles/projet-normandie/message-bundle/src",
            "ProjetNormandie\\PartnerBundle\\": "../../bundles/projet-normandie/partner-bundle/src",
            "ProjetNormandie\\TwitchBundle\\": "../../bundles/projet-normandie/twitch-bundle/src",
            "VideoGamesRecords\\CoreBundle\\": "../../bundles/video-games-records/core-bundle/src",
            "VideoGamesRecords\\DwhBundle\\": "../../bundles/video-games-records/dwh-bundle/src"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "console": "@php bin/console",
        "make:db": [
            "@console doctrine:database:create --if-not-exists",
            "@console doctrine:schema:drop --force --full-database",
            "@console doctrine:schema:create",
            "@console doctrine:fixtures:load -n"
        ],
        "make:db:test": [
            "@console doctrine:database:create --if-not-exists --env=test",
            "@console doctrine:schema:drop --force --full-database --env=test",
            "@console doctrine:schema:create --env=test",
            "@console d:fixtures:load --env=test -n"
        ],
        "lint:phpstan": "@php vendor/bin/phpstan",
        "lint:phpcs": "@php vendor/bin/phpcs src/ tests/ -p",
        "lint:phpcs:fix": "@php vendor/bin/phpcbf src/ tests/ -p",
        "lint": [
            "@lint:phpcs",
            "@lint:phpstan"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    }
}
